{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_dms_kafka_topic_quotas": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the Kafka instance to which the topic quotas belong.",
                "required": true
              },
              "keyword": {
                "type": "string",
                "description": "The keyword of the topic quota to be queried.",
                "optional": true
              },
              "quotas": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "consumer_byte_rate": "number",
                      "producer_byte_rate": "number",
                      "topic": "string"
                    }
                  ]
                ],
                "description": "All topic quotas that match the filter parameters.",
                "computed": true
              },
              "region": {
                "type": "string",
                "description": "The region where the topic quotas are located.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "Kafka",
            "name": "Distributed Message Service for Kafka",
            "catalog": "Middleware",
            "name_cn": "分布式消息服务 Kafka",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
