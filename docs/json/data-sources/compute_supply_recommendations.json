                "optional": true
              },
              "supply_recommendations": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "availability_zone_id": "string",
                      "flavor_id": "string",
                      "region_id": "string",
                      "score": "number"
                    }
                  ]
                ],
                "computed": true
              }
            },
            "block_types": {
