                                "min": {
                                  "type": "number",
                                  "optional": true
                                }
                              }
                            },
                            "nesting_mode": "list",
                            "max_items": 1
                          },
                          "memory_mb": {
                            "block": {
                              "attributes": {
                                "max": {
                                  "type": "number",
                                  "optional": true
                                },
                                "min": {
                                  "type": "number",
                                  "optional": true
                                }
                              }
                            },
                            "nesting_mode": "list",
                            "max_items": 1
                          },
                          "vcpu_count": {
                            "block": {
                              "attributes": {
                                "max": {
                                  "type": "number",
                                  "optional": true
                                },
                                "min": {
                                  "type": "number",
                                  "optional": true
                                }
