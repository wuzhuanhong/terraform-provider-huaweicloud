                    },
                    "region_id": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "nesting_mode": "list"
              },
              "option": {
                "block": {
                  "attributes": {
                    "enable_spot": {
                      "type": "string",
                      "optional": true
                    },
                    "result_granularity": {
                      "type": "string",
                      "optional": true
                    }
                  }
                },
                "nesting_mode": "list",
                "max_items": 1
