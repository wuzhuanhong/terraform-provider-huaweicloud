{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "data_source_schemas": {
        "huaweicloud_dms_kafka_instance_coordinators": {
          "block": {
            "attributes": {
              "coordinators": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "group_id": "string",
                      "host": "string",
                      "id": "number",
                      "port": "number"
                    }
                  ]
                ],
                "description": "The list of coordinators corresponding to all consumer groups.",
                "computed": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the Kafka instance to which the coordinators belong.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the coordinators are located.",
                "computed": true
              }
            }
          },
          "product": {
            "short": "Kafka",
            "name": "Distributed Message Service for Kafka",
            "catalog": "Middleware",
            "name_cn": "分布式消息服务 Kafka",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
