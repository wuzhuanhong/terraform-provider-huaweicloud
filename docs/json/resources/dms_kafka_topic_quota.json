{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_dms_kafka_topic_quota": {
          "block": {
            "attributes": {
              "consumer_byte_rate": {
                "type": "number",
                "description": "The consumer rate limit. The unit is byte/s.",
                "optional": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the Kafka instance to which the topic quota belongs.",
                "required": true
              },
              "producer_byte_rate": {
                "type": "number",
                "description": "The producer rate limit. The unit is byte/s.",
                "optional": true
              },
              "region": {
                "type": "string",
                "description": "The region where the topic quota is located.",
                "computed": true
              },
              "topic": {
                "type": "string",
                "description": "The name of the topic.",
                "required": true
              }
            }
          },
          "product": {
            "short": "Kafka",
            "name": "Distributed Message Service for Kafka",
            "catalog": "Middleware",
            "name_cn": "分布式消息服务 Kafka",
            "catalog_cn": "应用中间件"
          }
        }
      }
    }
  }
}
